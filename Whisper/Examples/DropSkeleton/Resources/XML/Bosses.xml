<?xml version = "1.0" encoding = "UTF-8" standalone = "no"?><!DOCTYPE BossList SYSTEM "XBosses.DTD" [	<!ENTITY AbstractBosses	SYSTEM "XAbstractBosses.XML">]><!-- This file contains the layout for all the COM bosses used by DropSkeleton. --><BossList>	&AbstractBosses;<!-- Application (this must be named "Application") -->	<Boss name = "Application" singleton = "true">						<Inherit name = "Base Drag and Drop App"/>		<Interface name = "IDropAppHelper" impl = "CDropAppHelper"/>	</Boss><!-- Window -->		<Boss name = "Progress Window">						<Inherit name = "DropApp Progress Window"/>	</Boss><!-- Controls -->		<Boss name = "Progress Bar">							<Inherit name = "Base Progress Bar"/>	</Boss>	<Boss name = "Push Button">							<Inherit name = "Base Push Button"/>		<Interface name = "IClickExtension, ICommandClickExtension" impl = "XCommandClickExtension"/>	</Boss>	<Boss name = "Root Control">							<Inherit name = "Base Root Control"/>	</Boss>	<Boss name = "Static Text">							<Inherit name = "Base Static Text"/>	</Boss><!-- Commands -->	<Boss name = "Callback Command">							<Interface name = "ICommand" impl = "XCommand"/>		<Interface name = "ICommandHelper, ICallbackCommand" impl = "XCallbackCommand"/>	</Boss></BossList>