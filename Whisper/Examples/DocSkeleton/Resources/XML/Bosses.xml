<?xml version = "1.0" encoding = "UTF-8" standalone = "no"?><!DOCTYPE BossList SYSTEM "XBosses.dtd" [	<!ENTITY AbstractBosses	SYSTEM "XAbstractBosses.xml">]><BossList>	&AbstractBosses;<!-- This file contains the layout for all the COM bosses used by DocSkeleton. --><!-- Application -->	<Boss name = "Application" singleton = "true">						<Inherit name = "Base Application"/>		<Inherit name = "Application Extras"/>				<Inherit name = "Document App"/>				<Inherit name = "Debug Extensions" platform = "mac" target = "debug"/>				<Interface name = "ITimer" impl = "XTimer"/>		<Interface name = "+IInstallMenuHandler" impl = "CAppMenuHandler"/>		<Interface name = "IInit" impl = "CAppInit"/>		<Interface name = "IInitMenuBar" impl = "CInitMenuBar" platform = "mac"/>	</Boss><!-- Commands -->	<Boss name = "Callback Command">							<Interface name = "ICommand" impl = "XCommand"/>		<Interface name = "ICommandHelper, ICallbackCommand" impl = "XCallbackCommand"/>	</Boss>	<Boss name = "Undo Callback Action">							<Inherit name = "Base Undoable Command"/>		<Interface name = "IUndoActionHelper, IUndoCallbackAction" impl = "XUndoCallbackAction"/>	</Boss>	<Boss name = "Deferred Error Command">							<Interface name = "ICommand" impl = "XCommand"/>		<Interface name = "ICommandHelper, IDeferredErrorCommand" impl = "XDeferredErrorCommand"/>	</Boss>	<Boss name = "Cut Action">							<Inherit name = "Base Cut Action"/>	</Boss>	<Boss name = "Copy Action">							<Inherit name = "Base Copy Action"/>	</Boss>	<Boss name = "Paste Action">							<Inherit name = "Base Paste Action"/>	</Boss>	<Boss name = "Clear Action">							<Inherit name = "Base Clear Action"/>	</Boss>	<Boss name = "Typing Action">							<Inherit name = "Base Typing Action"/>	</Boss>	<Boss name = "Delete Action">							<Inherit name = "Base Delete Action"/>	</Boss>	<Boss name = "Forward Delete Action">							<Inherit name = "Base Forward Delete Action"/>	</Boss> <!-- Controls -->		<Boss name = "Document Control">							<Inherit name = "Abstract Control"/>			<Interface name = "IUserControl" impl = "XUserControl"/>		<Interface name = "IParentChangedSize, IBinding" impl = "XDefaultChangedSize"/>		<Interface name = "IHelpText, ISetHelp" impl = "XDefaultHelp"/>		<Interface name = "IShowHelp" impl = "MBalloonHelp" platform = "classic_mac"/>		<Interface name = "IShowHelp" impl = "WHTMLHelp" platform = "win"/>		<Interface name = "IDrawHelper" impl = "CDrawDoc"/>			<Interface name = "IMouseDownHelper" impl = "CMouseHandler"/>			<Interface name = "IAdjustCursorHelper" impl = "CAdjustCursor"/>		</Boss>	<Boss name = "Push Button">							<Inherit name = "Base Push Button"/>		<Interface name = "IClickExtension, ICallbackClickExtension" impl = "XCallbackClickExtension"/>	</Boss>	<Boss name = "Root Control">							<Inherit name = "Base Root Control"/>	</Boss>	<Boss name = "Static Text">							<Inherit name = "Base Static Text"/>	</Boss>	<Boss name = "Int32 Text Box">							<Inherit name = "Base Int32 Text Box"/>		<Interface name = "IHelpText, ISetHelp" impl = "XDefaultHelp"/>		<Interface name = "IShowHelp" impl = "MBalloonHelp" platform = "classic_mac"/>		<Interface name = "IShowHelp" impl = "WHTMLHelp" platform = "win"/>	</Boss><!-- Dialogs -->		<Boss name = "Width Dialog">						<Inherit name = "Base Dialog Box"/>		<Interface name = "IDialog" impl = "CWidthDialog"/>	</Boss>	<Boss name = "Pose Dialog Box">						<Inherit name = "Base Pose Dialog Box"/>	</Boss><!-- Document -->		<Boss name = "Document">			<Inherit name = "Base Document"/>				<Interface name = "IPersistDoc" impl = "XPersistDoc"/>		<Interface name = "IDocInfo" impl = "CDocInfo"/>					<Interface name = "+IDocWriter" impl = "CXMLWriter"/>			<Interface name = "+IDocReader" impl = "CXMLReader"/>				<Interface name = "+IDocWriter" impl = "CBinaryWriter"/>			<Interface name = "+IDocReader" impl = "CBinaryReader"/>			</Boss><!-- Menus -->	<Boss name = "Menu">							<Interface name = "IMenu" impl = "XMenu"/>	</Boss>	<Boss name = "Context Menu">							<Interface name = "IContextMenu" impl = "XContextMenu"/>		<Interface name = "IMenu" impl = "XMenu"/>	</Boss><!-- Persistence -->		<Boss name = "XML Persist" singleton = "true">		<Interface name = "IReadXML" impl = "XReadXML"/>		<Interface name = "IWriteXML" impl = "XWriteXML"/>	</Boss><!-- Trackers -->	<Boss name = "Rubber Band Tracker">							<Interface name = "ITracker" impl = "XTracker"/>		<Interface name = "ITrackerHelper" impl = "XRubberBandTracker"/>	</Boss><!-- Windows -->		<Boss name = "MDI Frame Window">						<Inherit name = "Base MDI Frame Window" platform = "win"/>		<Inherit name = "Debug Extensions" platform = "win" target = "debug"/>				<Interface name = "IInitMenuBar" impl = "CInitMenuBar" platform = "win"/>	</Boss>	<Boss name = "Document Window">						<Inherit name = "Base Document Window"/>		<Interface name = "IDrawHelper" impl = "XNullDrawer"/>			<Interface name = "+IInstallMenuHandler" impl = "XWindowMenuHandler"/>		<Interface name = "+IInstallMenuHandler" impl = "CDocMenuHandler"/>					</Boss></BossList>