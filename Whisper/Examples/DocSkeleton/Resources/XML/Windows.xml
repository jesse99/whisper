<?xml version="1.0" encoding="UTF-8" standalone="no"?><!DOCTYPE Bosses SYSTEM "XControls.dtd"><!-- This file is used to initialize the interfaces on the window and dialog bosses. --><Bosses><!-- Document Window -->	<Boss name = "Document Window" id = "Document_Window">		<IWindow			name = "untitled">			<SWindowStyle				type = "document"				style = "WS_BORDER+WS_DLGFRAME+WS_SYSMENU+WS_THICKFRAME+WS_MINIMIZEBOX+WS_MAXIMIZEBOX"				style_ex = "0"				class_style = "CS_DBLCLKS"				draw_size_grip = "false"				window_class = "document"				attributes = "has_close_box+has_horz_zoom+has_vert_zoom+has_collapse_box+resizeable"				get_select_click = "false"/>		</IWindow>				<IDefaultPositionWindow position = "stagger_on_top_window"/>				<IGeometry			location = "6 42"			mac_size = "271 271"			win_size = "269 269"			max_size = "271 271"/>				<IHierarchy>			<Boss name = "Root Control">				<IControl					name = "Root Control"/>																<IRootControl/>				<IHierarchy>					<Boss name = "Document Control">						<IControl							name = "Document Control"/>														<IGeometry							location = "0 0"							size = "271 271"/>													<IBinding							bind_left = "true"							bind_right = "true"							bind_top = "true"							bind_bottom = "true"/>														<IUserControl/>														<ISetHelp							message = "Click and drag to create a new rectangle."/>					</Boss>				</IHierarchy>			</Boss>		</IHierarchy>	</Boss><!-- Width Dialog -->	<Boss name = "Width Dialog" id = "Width_Dialog">		<IWindow			name = "Pen Width">			<SWindowStyle				type = "modal"				style = "WS_CAPTION+WS_POPUP+WS_SYSMENU"				style_ex = "WS_EX_DLGMODALFRAME+WS_EX_CONTEXTHELP"				class_style = "CS_DBLCLKS+CS_NOCLOSE"				draw_size_grip = "false"				window_class = "moveable_modal"				attributes = ""				get_select_click = "false"/>		</IWindow>				<IDefaultPositionWindow position = "alert_position_on_top_window"/>		<IDialogBox/>				<IGeometry			location = "6 42"			mac_size = "196 83"			win_size = "196 89"/>				<IHierarchy>			<Boss name = "Root Control">				<IControl					name = "Root Control"/>																<IRootControl/>				<IHierarchy>					<Boss name = "Static Text">						<IControl							name = "Text Label"/>														<IGeometry							location = "10 12"							mac_size = "42 16"							win_size = "42 16"/>																					<IStaticText 							text = "Width:"/>					</Boss>					<Boss name = "Int32 Text Box">						<IControl							name = "Text"/>														<IGeometry							location = "60 12"							mac_size = "110 16"							win_size = "110 21"/>																					<ITextBox 							text = "1"/>																				<IInt32Range							min = "1"							max = "64"/>													<ISetHelp							message = "The width of the stroke around the boxes."/>					</Boss>					<Boss name = "Push Button">						<IControl							name = "cancel"/>														<IGeometry							location = "40 50"							mac_size = "58 20"							win_size = "58 20"/>																					<IPushButton 							text = "Cancel"/>					</Boss>					<Boss name = "Push Button">						<IControl							name = "ok"/>														<IGeometry							location = "120 50"							mac_size = "58 20"							win_size = "58 20"/>																					<IPushButton 							text = "OK"/>					</Boss>				</IHierarchy>			</Boss>		</IHierarchy>	</Boss></Bosses>