<?xml version = "1.0" encoding = "UTF-8" standalone = "no"?><!DOCTYPE BossList SYSTEM "XBosses.dtd"><!-- This file includes sample boss layouts for all the UI objects. Youcan use these bosses as is or you can create your own bosses if you needto customize the bosses or if you're concerned about code bloat. --><BossList><!-- Extensions (these are just for reference: you'll never actually inherit from one of these bosses) -->	<AbstractBoss name = "MenuExtensions">						<Group>			<Interface name = "IMenuExtension" impl = "XAssertExtension" target = "debug"/>			<Interface name = "IMenuExtension" impl = "XIntenseExtension" target = "debug"/>			<Interface name = "IMenuExtension" impl = "XObjectHeapExtension" target = "debug"/>			<Interface name = "IMenuExtension" impl = "XTraceExtension" target = "debug"/>			<Interface name = "IMenuExtension" impl = "XUnitTestExtension" target = "debug"/>			<Interface name = "IMenuExtension" impl = "XControlExtension" target = "debug"/>			<Interface name = "IMenuExtension" impl = "XInputTargetExtension" target = "debug"/>			<Interface name = "IMenuExtension" impl = "MMacsbugExtension" target = "debug" platform = "mac"/>			<Interface name = "IMenuExtension" impl = "MZoneRangerExtension" target = "debug" platform = "mac"/>			<Interface name = "IMenuExtension" impl = "MSIOUXExtension" target = "debug" platform = "mac"/>			<Interface name = "IMenuExtension" impl = "MSystemInfoExtension" target = "debug" platform = "mac"/>			<Interface name = "IMenuExtension" impl = "WSystemInfoExtension" target = "debug" platform = "win"/>		</Group>	</AbstractBoss>	<AbstractBoss name = "MouseExtensions">						<Group>			<Interface name = "IClickExtension, IMenuCommandClickExtension" impl = "XMenuCommandClickExtension"/>			<Interface name = "IClickExtension, ICommandClickExtension" impl = "XCommandClickExtension"/>			<Interface name = "IClickExtension, ICallbackClickExtension" impl = "XCallbackClickExtension"/>		</Group>	</AbstractBoss>	<AbstractBoss name = "DrawExtensions">						<Group>			<Interface name = "IDrawExtension, IFillExtension"   impl = "XEraseExtension"/>			<Interface name = "IDrawExtension, IStrokeExtension" impl = "XLineLeftExtension"/>			<Interface name = "IDrawExtension, IStrokeExtension" impl = "XLineRightExtension"/>			<Interface name = "IDrawExtension, IStrokeExtension" impl = "XLineTopExtension"/>			<Interface name = "IDrawExtension, IStrokeExtension" impl = "XLineBottomExtension"/>			<Interface name = "IDrawExtension, IStrokeExtension" impl = "XFrameExtension"/>			<Interface name = "IDrawExtension, IStrokeExtension" impl = "XShadowedFrameExtension"/>		</Group>	</AbstractBoss><!-- Application -->	<Boss name = "Application" singleton = "true">			<!-- must have this name -->				<Interface name = "IApplication" impl = "XApplication"/>		<Interface name = "IEventDispatcher" impl = "MEventDispatcher" platform = "mac"/>		<Interface name = "IMenuHandler" impl = "XAppMenuHandler"/>		<Interface name = "IWindowMgr" impl = "WindowMgr"/>		<Interface name = "IHierarchy" impl = "XHierarchy"/>		<Interface name = "IFocus" impl = "Focus"/>				<!-- optional interfaces -->		<Interface name = "IStringTableLoader" impl = "XStringTableLoader"/>						<Interface name = "IMenuBar" impl = "MenuBar" platform = "mac"/>		<Interface name = "IInitMenuBar" impl = "XInitMenuBar" platform = "mac"/>		<Interface name = "IActiveMenuBar" impl = "XActiveMenuBar"/>		<Interface name = "IIdler" impl = "XIdler"/>						<Interface name = "ITimer" impl = "XTimer"/>		<Interface name = "ICommandQueue" impl = "XCommandQueue"/>		<Interface name = "IActiveUndoContext" impl = "XActiveUndoContext"/>		<Interface name = "IUndoContext" impl = "XUndoContext"/>		<Interface name = "IMenuExtensible" impl = "XMenuExtensible"/>		<Interface name = "IKeyExtensible" impl = "XKeyExtensible"/>	</Boss><!-- Persistence -->	<Boss name = "XML Persist" singleton = "true">			<!-- must have this name -->		<Interface name = "IPersistXML" impl = "XPersistXML"/>	</Boss><!-- Drag and Drop App Progress Window -->	<Boss name = "Progress Window">						<Interface name = "IWindow" impl = "Window"/>			<Interface name = "IDraw" impl = "DrawWindow"/>			<Interface name = "IGeometry" impl = "WindowGeometry"/>			<Interface name = "IHierarchy" impl = "XHierarchy"/>		<Interface name = "IFocus" impl = "XWindowFocus"/>		<Interface name = "IDropAppProgress" impl = "XDropAppProgress"/>	</Boss><!-- Window -->	<Boss name = "Window">						<Interface name = "IWindow" impl = "Window"/>			<Interface name = "IDraw" impl = "DrawWindow"/>			<Interface name = "IGeometry" impl = "WindowGeometry"/>			<Interface name = "IMenuHandler" impl = "XWindowMenuHandler"/>		<Interface name = "IHierarchy" impl = "XHierarchy"/>		<Interface name = "IFocus" impl = "XWindowFocus"/>				<!-- optional interfaces -->		<Interface name = "IKeyHandler" impl = "XWindowKeyHandler"/>						<!-- handles tabbing to next control -->		<Interface name = "IMouseHandler" impl = "MWindowMouseHandler" platform = "mac"/>	<!-- need this if the window has controls -->		<Interface name = "IMenuBar" impl = "MMenuBar" platform = "win"/>		<Interface name = "IInitMenuBar" impl = "XInitMenuBar" platform = "win"/>		<Interface name = "IUndoContext" impl = "XUndoContext"/>				<Interface name = "IHelpText, ISetHelpText" impl = "XDefaultHelpText"/>		<Interface name = "IShowHelp" impl = "MBalloonHelp" platform = "classic_mac"/>		<Interface name = "IMenuExtensible" impl = "XMenuExtensible"/>		<Interface name = "IMouseExtensible" impl = "XMouseExtensible"/>		<Interface name = "IKeyExtensible" impl = "XKeyExtensible"/>		<Interface name = "IDrawExtensible" impl = "XDrawExtensible"/>	</Boss><!-- Menus -->	<Boss name = "Menu">							<Interface name = "IMenu" impl = "Menu"/>		<!-- optional -->		<Interface name = "IXMLMenu" impl = "XXMLMenu"/>				</Boss>	<Boss name = "Context Menu">							<Interface name = "IContextMenu" impl = "ContextMenu"/>		<Interface name = "IMenu" impl = "Menu"/>		<!-- optional -->		<Interface name = "IXMLMenu" impl = "XXMLMenu"/>				</Boss><!-- Commands -->	<Boss name = "Deferred Menu Command">							<Interface name = "ICommand, IDeferredMenuCommand" impl = "XDeferredMenuCommand"/>	</Boss>	<Boss name = "Deferred Error Command">							<Interface name = "ICommand, IDeferredErrorCommand" impl = "XDeferredErrorCommand"/>	</Boss>	<Boss name = "Callback Command">							<Interface name = "ICommand, ICallbackCommand" impl = "XCallbackCommand"/>	</Boss><!-- Undo Actions -->	<Boss name = "Multiple Undo Action">							<Interface name = "IUndoAction, IMultipleUndoAction" impl = "XMultipleUndoAction"/>		<Interface name = "ICommand" impl = "XUndoCommand"/>	</Boss>	<Boss name = "Undo Callback Action">							<Interface name = "IUndoAction, IUndoCallbackAction" impl = "XMultipleUndoAction"/>		<Interface name = "ICommand" impl = "XUndoCallbackAction"/>	</Boss><!-- Abstract Controls -->	<AbstractBoss name = "Base Control">							<Interface name = "IControl" impl = "Control"/>			<Interface name = "IDraw" impl = "DrawControl"/>			<Interface name = "IGeometry" impl = "ControlGeometry"/>			<Interface name = "IHierarchy" impl = "XHierarchy"/>				<!-- optional interfaces -->		<Interface name = "IMouseHandler" impl = "XControlMouseHandler"/>		<Interface name = "IHelpText, ISetHelpText, ISetDisabledHelp" impl = "XControlHelp"/>		<Interface name = "IShowHelp" impl = "MBalloonHelp" platform = "classic_mac"/>		<Interface name = "IMouseExtensible" impl = "XMouseExtensible"/>		<Interface name = "IDrawExtensible" impl = "XDrawExtensible"/>	</AbstractBoss>	<AbstractBoss name = "Value Control">							<Inherit name = "Base Control"/>		<Interface name = "IValueControl" impl = "ValueControl"/>				<!-- optional interfaces -->		<Interface name = "IHelpText, ISetHelpText, ISetDisabledHelp, ISetCheckedHelp" impl = "XButtonHelp"/>	</AbstractBoss>	<AbstractBoss name = "Targetable Control">							<Inherit name = "Base Control"/>		<Interface name = "IFocus" impl = "ControlFocus"/>		<Interface name = "IKeyHandler" impl = "ControlKeyHandler"/>						<!-- optional interfaces -->		<Interface name = "IKeyExtensible" impl = "XKeyExtensible"/>	</AbstractBoss><!-- Concrete Controls -->	<Boss name = "Group Control">							<Inherit name = "Group Control"/>		<Interface name = "IGroupControl" impl = "GroupControl"/>	</Boss>	<Boss name = "Progress Bar">							<Inherit name = "Value Control"/>		<Interface name = "IProgressBar" impl = "ProgressBar"/>		<Interface name = "IValueControl" impl = "M16BitValueControl" platform = "classic_mac"/>	<!-- progress bar doesn't handle 32-bit values... -->		<Interface name = "IValueControl" impl = "WProgressValueControl" platform = "win"/>		</Boss>	<Boss name = "Push Button">							<Inherit name = "Base Control"/>		<Interface name = "IPushButton" impl = "PushButton"/>		<Interface name = "IText" impl = "ControlText"/>		<Interface name = "IMouseExtensible" impl = "XMouseExtensible"/>	</Boss>	<Boss name = "Root Control">							<Inherit name = "Base Control"/>		<Interface name = "IRootControl" impl = "RootControl"/>		<Interface name = "IGeometry" impl = "MRootGeometry" platform = "mac"/>			<Interface name = "IParentChangedSize, IBinding" impl = "XDefaultChangedSize" platform = "win"/>	</Boss>	<Boss name = "Static Text">							<Inherit name = "Base Control"/>		<Interface name = "IStaticText, IText" impl = "MStaticText" platform = "mac"/>		<Interface name = "IStaticText" impl = "WStaticText" platform = "win"/>		<Interface name = "IText" impl = "ControlText" platform = "win"/>	</Boss></BossList>