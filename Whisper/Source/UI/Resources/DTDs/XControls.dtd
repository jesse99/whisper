<?xml version = "1.0" encoding = "UTF-8"?><!ENTITY % Windows SYSTEM "XWindows.dtd">%Windows;<!-- this file defines the elements and attributes used to generate controls from XML --><!ENTITY % ALIGNMENT "(left_top | center_top | right_top | default)"><!ENTITY % PUSHPROC  "(kControlPushButtonProc | kControlPushButLeftIconProc | kControlPushButRightIconProc)"><!ENTITY % BEVELPROC "(kControlBevelButtonSmallBevelProc | kControlBevelButtonNormalBevelProc | kControlBevelButtonLargeBevelProc)"><!ENTITY % BEVELBEHAVIOR "(resets | toggles | sticks)"><!ENTITY % BEVELALIGNMENT "(left | center | right | system)"><!-- ****************** XFont ****************** --><!ELEMENT XFont	 			EMPTY>						<!ATTLIST XFont				font 			%STRING;	"system"><!ATTLIST XFont				size 			%INTEGER;	"12"><!ATTLIST XFont				style 			%STRING;	"plain">		<!-- can be plain or sum of bold, italic, and underline --><!ATTLIST XFont				color 			%COLOR;		"0.0 0.0 0.0"><!-- ****************** IControl ****************** --><!ELEMENT IControl			EMPTY>					<!ATTLIST IControl			name 			%STRING;	""><!ATTLIST IControl			visible 		%BOOL;		"true"><!ATTLIST IControl			enabled			%BOOL;		"true"><!-- ****************** IBevelButton ****************** --><!ELEMENT IBevelButton		EMPTY>						<!ATTLIST IBevelButton		text 			%STRING;			""><!ATTLIST IBevelButton		text_align		%BEVELALIGNMENT; 	"center"><!ATTLIST IBevelButton		icon_id 		%INTEGER;		 	"0"><!ATTLIST IBevelButton		image_uri 		%STRING;		 	""><!ATTLIST IBevelButton		behavior		%BEVELBEHAVIOR;	 	#REQUIRED><!ATTLIST IBevelButton		style 			%STRING;			"WS_CHILD+WS_VISIBLE+WS_TABSTOP+BS_PUSHLIKE"><!ATTLIST IBevelButton		proc_id			%BEVELPROC; 		"kControlBevelButtonNormalBevelProc"><!-- ******************* ICheckBox ****************** --><!ELEMENT ICheckBox			EMPTY>						<!ATTLIST ICheckBox			text 			%STRING;	#REQUIRED><!ATTLIST ICheckBox			style 			%STRING;	"WS_CHILD+WS_VISIBLE+WS_TABSTOP+BS_3STATE"><!ATTLIST ICheckBox			proc_id			%STRING; 	"kControlCheckBoxProc"><!-- ******************* IColorSwatch ****************** --><!ELEMENT IColorSwatch		EMPTY>						<!ATTLIST IColorSwatch		prompt 			%STRING;	#REQUIRED><!ATTLIST IColorSwatch		color 			%COLOR;		"1.0 1.0 1.0"><!-- ******************* IGroupBox ****************** --><!ELEMENT IGroupBox			EMPTY>						<!ATTLIST IGroupBox			text 			%STRING;	#REQUIRED><!ATTLIST IGroupBox			style 			%STRING;	"WS_CHILD+WS_VISIBLE+BS_GROUPBOX"><!ATTLIST IGroupBox			proc_id			%STRING; 	"kControlGroupBoxTextTitleProc"><!-- ****************** IGroupControl ****************** --><!ELEMENT IGroupControl		EMPTY>						<!ATTLIST IGroupControl		style 			%STRING;	"WS_CHILD+WS_VISIBLE+SS_NOTIFY"><!ATTLIST IGroupControl		proc_id	 		%STRING;	"kControlUserPaneProc"><!-- ******************** IListBox ******************** --><!ELEMENT IListBox			(ListEntry*)>						<!ELEMENT ListEntry			(#PCDATA)><!ATTLIST IListBox			style 			%STRING;	"WS_CHILD+WS_VISIBLE+WS_TABSTOP+WS_BORDER"><!ATTLIST IListBox			proc_id	 		%STRING;	"kControlListBoxProc"><!ATTLIST IListBox			has_vert_scrollbar	%BOOL;	"true"><!ATTLIST IListBox			has_horz_scrollbar	%BOOL;	"false"><!ATTLIST IListBox			multiple_selections %BOOL;	#REQUIRED><!ATTLIST IListBox			use_type_ahead		%BOOL;	"true"><!ATTLIST IListBox			has_grow_box		%BOOL;	"false">	<!-- Mac only --><!-- ****************** IProgressBar ****************** --><!ELEMENT IProgressBar		EMPTY>						<!ATTLIST IProgressBar		indeterminate	%BOOL;		"false"><!ATTLIST IProgressBar		style 			%STRING;	"WS_CHILD+WS_VISIBLE"><!ATTLIST IProgressBar		proc_id	 		%STRING;	"kControlProgressBarProc"><!-- ****************** IPushButton ****************** --><!ELEMENT IPushButton		EMPTY>						<!ATTLIST IPushButton		text 			%STRING;	#REQUIRED><!ATTLIST IPushButton		style 			%STRING;	"WS_CHILD+WS_VISIBLE+WS_TABSTOP+BS_PUSHBUTTON"><!ATTLIST IPushButton		proc_id			%PUSHPROC; 	"kControlPushButtonProc"><!-- ******************* IRadioButton ****************** --><!ELEMENT IRadioButton		EMPTY>						<!ATTLIST IRadioButton		text 			%STRING;	#REQUIRED><!ATTLIST IRadioButton		family 			%STRING;	#REQUIRED><!ATTLIST IRadioButton		style 			%STRING;	"WS_CHILD+WS_VISIBLE+WS_TABSTOP+BS_RADIOBUTTON"><!ATTLIST IRadioButton		proc_id			%STRING; 	"kControlRadioButtonProc"><!-- ****************** IRootControl ****************** --><!ELEMENT IRootControl		EMPTY>						<!-- ****************** IScrollableControl ****************** --><!ELEMENT IScrollableControl EMPTY>						<!ATTLIST IScrollableControl adjust_origin		%BOOL;	"true"><!ATTLIST IScrollableControl reconcile_overhang	%BOOL;	"true"><!-- ****************** IScrollBar ****************** --><!ELEMENT IScrollBar		EMPTY>						<!ATTLIST IScrollBar		page_delta		%INTEGER;	"1"><!ATTLIST IScrollBar		arrow_delta		%INTEGER;	"1"><!ATTLIST IScrollBar		style 			%STRING;	"WS_CHILD+WS_VISIBLE">			<!-- SBS_HORZ and SBS_VERT will be added depending on frame orientation --><!ATTLIST IScrollBar		proc_id	 		%STRING;	"kControlScrollBarLiveProc"><!-- ****************** IScrollerControl ****************** --><!ELEMENT IScrollerControl	EMPTY>						<!ATTLIST IScrollerControl	has_horz_bar 	%BOOL;		"true"><!ATTLIST IScrollerControl	has_vert_bar 	%BOOL;		"true"><!ATTLIST IScrollerControl	left_indent		%INTEGER;	"0"><!ATTLIST IScrollerControl	right_indent	%INTEGER;	"0"><!ATTLIST IScrollerControl	top_indent		%INTEGER;	"0"><!ATTLIST IScrollerControl	bottom_indent	%INTEGER;	"0"><!ATTLIST IScrollerControl	fills_window 	%BOOL;		"true"><!ATTLIST IScrollerControl	scroll_units	%POINT;		"1 1"><!-- ******************* ISeperator ****************** --><!ELEMENT ISeperator		EMPTY>						<!ATTLIST ISeperator		style 			%STRING;	"WS_CHILD+WS_VISIBLE+SS_ETCHEDHORZ">	<!-- automatically switches to vertical if the control is taller than it is wide --><!ATTLIST ISeperator		proc_id	 		%STRING;	"kControlSeparatorLineProc"><!-- ****************** IStaticText ****************** --><!ELEMENT IStaticText		(XFont?)>						<!ATTLIST IStaticText		text 			%STRING;	""><!ATTLIST IStaticText		alignment 		%ALIGNMENT;	"default"><!ATTLIST IStaticText		style 			%STRING;	"WS_CHILD+WS_VISIBLE+SS_LEFT"><!ATTLIST IStaticText		proc_id	 		%STRING;	"kControlStaticTextProc"><!-- ****************** IUserControl ****************** --><!ELEMENT IUserControl		EMPTY>						<!ATTLIST IUserControl		style 			%STRING;	"WS_CHILD+WS_VISIBLE+SS_NOTIFY+SS_OWNERDRAW"><!ATTLIST IUserControl		proc_id	 		%STRING;	"kControlUserPaneProc"><!-- ****************** ITextBox ****************** --><!ELEMENT ITextBox			EMPTY>						<!ATTLIST ITextBox			text 			%STRING;	""><!ATTLIST ITextBox			style 			%STRING;	"WS_CHILD+WS_VISIBLE+WS_TABSTOP+ES_AUTOHSCROLL+ES_AUTOVSCROLL+ES_LEFT+ES_MULTILINE"><!ATTLIST ITextBox			proc_id	 		%STRING;	"kControlEditTextProc"><!-- ****************** IValueControl ****************** --><!ELEMENT IValueControl		EMPTY>					<!ATTLIST IValueControl		value	 		%INTEGER;	"0"><!ATTLIST IValueControl		min_value		%INTEGER;	"0"><!ATTLIST IValueControl		max_value		%INTEGER;	"0">