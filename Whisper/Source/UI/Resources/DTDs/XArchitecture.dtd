<?xml version = "1.0" encoding = "UTF-8"?><!ENTITY % Types SYSTEM "XTypes.dtd">%Types;<!-- this file defines the elements and attributes used to stream in various architecture interfaces --><!-- ****************** Boss ****************** --><!ELEMENT Boss		 		ANY>									<!-- ANY since we allow for client interfaces -->	<!ATTLIST Boss				name			%STRING; 	#REQUIRED>		<!ATTLIST Boss				id				ID		 	#IMPLIED>		<!-- ****************** Bosses ****************** --><!ELEMENT Bosses	 		(LocaleTable?, Boss*)><!-- ****************** IBinding ****************** --><!ELEMENT IBinding			EMPTY>						<!ATTLIST IBinding			bind_left		%BOOL;		"false"><!ATTLIST IBinding			bind_right		%BOOL;		"false"><!ATTLIST IBinding			bind_top		%BOOL;		"false"><!ATTLIST IBinding			bind_bottom		%BOOL;		"false"><!-- ****************** IBrushDrawer ****************** --><!ELEMENT IBrushDrawer		EMPTY>						<!ATTLIST IBrushDrawer		color			%COLOR;		#REQUIRED><!-- ****************** IDoubleRange ****************** --><!ELEMENT IDoubleRange		EMPTY>						<!ATTLIST IDoubleRange		min				%FLOAT;		#REQUIRED><!ATTLIST IDoubleRange		max				%FLOAT;		#REQUIRED><!-- ****************** IGeometry ****************** --><!ELEMENT IGeometry			EMPTY>						<!ATTLIST IGeometry			location		%POINT;		"0 0"><!ATTLIST IGeometry			size 			%POINT;		"32 32"><!ATTLIST IGeometry			min_size 		%POINT;		"16 16"><!ATTLIST IGeometry			max_size 		%POINT;		"16000 16000"><!ATTLIST IGeometry			mac_loc			%POINT;		#IMPLIED>		<!-- if present these will override location and size --><!ATTLIST IGeometry			win_loc			%POINT;		#IMPLIED><!ATTLIST IGeometry			mac_size 		%POINT;		#IMPLIED><!ATTLIST IGeometry			win_size 		%POINT;		#IMPLIED><!-- ****************** IHierarchy ****************** --><!ELEMENT IHierarchy		(Boss*)>						<!-- ****************** IInt32Range ****************** --><!ELEMENT IInt32Range		EMPTY>						<!ATTLIST IInt32Range		min				%INTEGER;	#REQUIRED><!ATTLIST IInt32Range		max				%INTEGER;	#REQUIRED><!-- ****************** ISetCheckedHelp ****************** --><!ELEMENT ISetCheckedHelp	EMPTY>						<!ATTLIST ISetCheckedHelp	message			%STRING;	""><!-- ****************** ISetDisabledHelp ****************** --><!ELEMENT ISetDisabledHelp	EMPTY>						<!ATTLIST ISetDisabledHelp	message			%STRING;	""><!-- ****************** ISetHelp ****************** --><!ELEMENT ISetHelp			EMPTY>						<!ATTLIST ISetHelp			message			%STRING;	""><!-- ****************** IText ****************** --><!ELEMENT IText				EMPTY>						<!ATTLIST IText				text			%STRING;	#REQUIRED><!-- ****************** IUInt32Range ****************** --><!ELEMENT IUInt32Range		EMPTY>						<!ATTLIST IUInt32Range		min				%INTEGER;	#REQUIRED><!ATTLIST IUInt32Range		max				%INTEGER;	#REQUIRED><!-- ****************** IValidateText ****************** --><!ELEMENT IValidateText		EMPTY>						<!ATTLIST IValidateText		min_chars		%INTEGER;	"1"><!ATTLIST IValidateText		max_chars		%INTEGER;	"256">